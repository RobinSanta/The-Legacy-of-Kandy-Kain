<h1>A New Arm To Defend Nosgoth</h1>

<%= form_for(@weapon) do |a| %>
    <%= a.label :name %>
    <%= a.text_field :name %><br><br>

    <%= a.label :damage %>
    <%= a.number_field :damage %><br><br>

    <%= a.label :speed %>
    <%= a.number_field :speed %><br><br>

    <%= a.label :description %>
    <%= a.text_field :description %><br><br>

    <%= a.submit "Add Weapon" %>
<% end %>