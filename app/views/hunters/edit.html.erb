<h1>How Your Skills Have Sharpened</h1>

<%= form_for(@hunter) do |a| %>
    <%=a.label :name %>
    <%=a.text_field :name %><br><br>

    <%=a.label :hp %>
    <%=a.number_field :hp %><br><br>

    <%=a.label :speed %>
    <%=a.number_field :speed %><br><br>

    <%=a.submit "Return To The Fold" %>
<% end %>